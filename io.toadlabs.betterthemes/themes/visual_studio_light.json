{
	"name": "Visual Studio Light+",
	"type": "dark",
	
	// from https://github.com/tomasiser/vim-code-dark
	// updated to light theme
	// colour names may now be inconsistent
	"variables": {
		"background": "#FFF",
		"grey": "#808080",
		"foreground": "#000",
		"lightBlue": "#001080",
		"blue": "#00F",
		"blueGreen": "#267F99",
		"green": "#008000",
		"lightGreen": "#098658",
		"yellow": "#795E26",
		"yellowOrange": "#800000",
		"orange": "#A31515",
		"lightRed": "#811F3F",
		"red": "#CD3131",
		"pink": "#AF00DB",
		"violet": "#811f3f"
	},
	"background": "@background",
	"foreground": "@foreground",
	"selectionBackground": "#ADD6FF",
	"currentLine": "#EEEEEE",
	"lineNumbers": "#237893",
	"comment": "@green",
	"keyword": "@blue",
	"number": "@lightGreen",
	"string": "@orange",
	"class": "@blueGreen",
	"method": "@yellow",
	"field": "@lightBlue",
	"constant": "#0070C1",
	"localVariable": "@lightBlue",
	"annotationKey": "@yellow",
	"deprecated": {
		"color": "@grey",
		"strikethrough": true
	},
	"occurrence": "#D5D5D5",
	"docTag": "@blue",
	"docLink": "&docTag",
	"docXmlTag": "&docTag",
	"key": "@lightBlue",
	"xmlTag": "@blue",
	"xmlAttribute": "@lightBlue"
}